{
  "manifest_version": 3,
  "name": "My Custom Homepage Extension",
  "version": "1.0",
  "description": "A custom homepage extension with multiple widgets.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
